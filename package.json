{
  "name": "solace-event-handler",
  "version": "1.0.0",
  "description": "Pub/Sub to Solace Event Handler",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "test": "nodemon",
    "build": "rm -rf dist/ && prettier src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:up": "rm -rf dist && tsup ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raprins/solace-event-handler.git"
  },
  "keywords": [
    "MQTT",
    "AMQP",
    "Messaging"
  ],
  "author": "Princis RAKOTOMANGA",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mqtt": "^2.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@raprincis/utilities": "^0.1.0",
    "mqtt": "^5.0.4",
    "rhea": "^3.0.2"
  }
}
